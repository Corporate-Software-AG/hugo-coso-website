.has-shadow {
  padding-bottom: 40px;
  position: relative;
  z-index: 1; }
  .has-shadow::after {
    position: absolute;
    content: "";
    height: calc(100% - 40px);
    width: calc(100% - 40px);
    bottom: 0;
    background-color: #E0E0E0;
    z-index: -1;
    border-radius: 5px; }
  .has-shadow.shadow-light::after {
    background-color: #f6f6f6; }
